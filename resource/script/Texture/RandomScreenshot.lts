function Texture Get ()
  var path "D:/lt"
  while path.IsDirectory
    path += "/"
    var contents path.ListDir
    var selection (contents.Get (Int_Random contents.Size))
    path += selection

  switch
    true
      Texture_LoadFrom
        Location_File path
    otherwise
      Texture/Filters:Artistic
        Texture_LoadFrom
          Location_File path
